sted_2","_createCommentVNode"],"sources":["/home/ali/BugTrackerProject/Frontend/bugtrackingfrontend/src/components/SingleBug.vue"],"sourcesContent":["<template>\n  <div class=\"bug-details\">\n    <DataTable :value=\"bugs\" tableStyle=\"min-width: 50rem\">\n      <Column field=\"bug_description\" header=\"Details\" :body=\"detailsTemplate\"></Column>\n      <Column field=\"bug_type\" header=\"Status\"></Column>\n      <Column field=\"bug_project.end_date\" header=\"Due Date\"></Column>\n      <Column field=\"bug_user.user_name\" header=\"Assigned To\"></Column>\n      <Column header=\"Action\" :body=\"actionTemplate\"></Column>\n    </DataTable>\n    <div v-if=\"!bugs.length\" class=\"no-bugs-message\">\n      No bugs available for this project.\n    </div>\n  </div>\n</template>\n\n<script>\nimport { DataTable } from 'primevue/datatable';\nimport { Column } from 'primevue/column';\nimport { updateBugStatus, deleteBug } from '../servicesfile/service';\n\nexport default {\n  name: \"SingleBug\",\n  components: {\n    DataTable,\n    Column\n  },\n  props: {\n    bugs: {\n      type: Array,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      activeDropdown: null,\n    };\n  },\n  methods: {\n    toggleDropdown(index) {\n      this.activeDropdown = this.activeDropdown === index ? null : index;\n    },\n    async changeStatus(bug, status) {\n      const success = await updateBugStatus(bug.id, status);\n      if (success) {\n        this.$emit('update-bugs'); // Emit event after status update\n      }\n      this.activeDropdown = null;\n    },\n    async deleteBug(bug) {\n      const success = await deleteBug(bug.id);\n      if (success) {\n        this.$emit('update-bugs'); // Emit event after bug deletion\n      }\n      this.activeDropdown = null;\n    },\n    detailsTemplate(bug) {\n      return (\n        <div>\n          <p>{bug.bug_description || 'No details available'}</p>\n        </div>\n      );\n    },\n    actionTemplate(bug) {\n      retur