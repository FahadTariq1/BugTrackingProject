{"ast":null,"code":"import axios from 'axios'; // Import Axios\n\nexport default {\n  name: 'RightSideSignup',\n  data() {\n    return {\n      form: {\n        name: '',\n        mobile: '',\n        email: '',\n        password: ''\n      },\n      userType: '',\n      // Add userType data property\n      successMessage: '',\n      errorMessage: ''\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      // Clear previous messages\n      this.successMessage = '';\n      this.errorMessage = '';\n\n      // Validate form fields\n      if (!this.form.name || !this.form.mobile || !this.form.email || !this.form.password || !this.userType) {\n        this.errorMessage = 'Please fill in all required fields, including user type.';\n        return;\n      }\n      try {\n        const response = await axios.post('http://127.0.0.1:8000/BugUsers/signup/', {\n          user_name: this.form.name,\n          user_email: this.form.email,\n          user_password: this.form.password,\n          user_type: this.userType\n        });\n        this.successMessage = 'You have successfully signed up!';\n        console.log('Response:', response.data);\n        // Optionally, redirect to a login page or another page\n        // this.$router.push('/login');\n      } catch (error) {\n        if (error.response) {\n          console.error('Error response data:', error.response.data);\n          console.error('Error response status:', error.response.status);\n          console.error('Error response headers:', error.response.headers);\n          this.errorMessage = 'Error: ' + (error.response.data.user_email ? error.response.data.user_email[0] : 'An error occurred');\n        } else if (error.request) {\n          console.error('Error request data:', error.request);\n          this.errorMessage = 'Error: No response received from server';\n        } else {\n          console.error('Error message:', error.message);\n          this.errorMessage = 'Error: ' + error.message;\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","form","mobile","email","password","userType","successMessage","errorMessage","methods","handleSubmit","response","post","user_name","user_email","user_password","user_type","console","log","error","status","headers","request","message"],"sources":["/home/ali/BugTrackerProject/Frontend/bugtrackingfrontend/src/components/RightSideSign.vue"],"sourcesContent":["<template>\n  <div class=\"RightSide\">\n    <div class=\"main text\">\n      <div class=\"Signin\">\n        <b><h2>Sign Up</h2></b>\n        <label>Please fill in the information below</label>\n      </div>\n      <div class=\"form\">\n        <form @submit.prevent=\"handleSubmit\">\n          <div v-if=\"successMessage\" class=\"success-message\">{{ successMessage }}</div>\n          <div v-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</div>\n          <div class=\"form-group\">\n            <div class=\"input-container\">\n              <i class=\"fas fa-user icon\"></i>\n              <input type=\"text\" id=\"name\" v-model=\"form.name\" placeholder=\"Enter your name\" />\n            </div>\n          </div>\n          <div class=\"form-group\">\n            <div class=\"input-container\">\n              <i class=\"fas fa-mobile-alt icon\"></i>\n              <input type=\"tel\" id=\"mobile\" v-model=\"form.mobile\" placeholder=\"Enter your mobile number\" />\n            </div>\n          </div>\n          <div class=\"form-group\">\n            <div class=\"input-container\">\n              <i class=\"fas fa-envelope icon\"></i>\n              <input type=\"email\" id=\"email\" v-model=\"form.email\" placeholder=\"Enter your email\" />\n            </div>\n          </div>\n          <div class=\"form-group\">\n            <div class=\"input-container\">\n              <i class=\"fas fa-lock icon\"></i>\n              <input type=\"password\" id=\"password\" v-model=\"form.password\" placeholder=\"Enter your password\" />\n            </div>\n          </div>\n          <div class=\"form-group\">\n            <div class=\"input-container\">\n              <i class=\"fas fa-user-tag icon\"></i>\n              <input type=\"text\" id=\"userType\" v-model=\"userType\" placeholder=\"Enter user type (e.g., Developer, QA)\" />\n            </div>\n          </div>\n          <button type=\"submit\"><b>Sign Up</b> <i class=\"fas fa-arrow-right\"></i></button>\n          <hr class=\"dotted-line\" />\n        </form>\n      </div>\n      <div class=\"AlreadyAccount\"></div>\n      <label>Already have an account?</label> <router-link to=\"/Login\" class=\"login-link\"> Log in to your account</router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'; // Import Axios\n\nexport default {\n  name: 'RightSideSignup',\n  data() {\n    return {\n      form: {\n        name: '',\n        mobile: '',\n        email: '',\n        password: '',\n      },\n      userType: '', // Add userType data property\n      successMessage: '',\n      errorMessage: '',\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      // Clear previous messages\n      this.successMessage = '';\n      this.errorMessage = '';\n\n      // Validate form fields\n      if (!this.form.name || !this.form.mobile || !this.form.email || !this.form.password || !this.userType) {\n        this.errorMessage = 'Please fill in all required fields, including user type.';\n        return;\n      }\n\n      try {\n        const response = await axios.post('http://127.0.0.1:8000/BugUsers/signup/', {\n          user_name: this.form.name,\n          user_email: this.form.email,\n          user_password: this.form.password,\n          user_type: this.userType,\n        });\n        this.successMessage = 'You have successfully signed up!';\n        console.log('Response:', response.data);\n        // Optionally, redirect to a login page or another page\n        // this.$router.push('/login');\n      } catch (error) {\n        if (error.response) {\n          console.error('Error response data:', error.response.data);\n          console.error('Error response status:', error.response.status);\n          console.error('Error response headers:', error.response.headers);\n          this.errorMessage = 'Error: ' + (error.response.data.user_email ? error.response.data.user_email[0] : 'An error occurred');\n        } else if (error.request) {\n          console.error('Error request data:', error.request);\n          this.errorMessage = 'Error: No response received from server';\n        } else {\n          console.error('Error message:', error.message);\n          this.errorMessage = 'Error: ' + error.message;\n        }\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.success-message {\n  color: green;\n  margin-bottom: 15px;\n  font-weight: bold;\n}\n\n.error-message {\n  color: red;\n  margin-bottom: 15px;\n  font-weight: bold;\n}\n\n.RightSide {\n  position: absolute;\n  right: 0;\n  top: 0;\n  bottom: 0;\n  width: 50%;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 20px;\n  box-sizing: border-box;\n  background-color: #f9f9f9; \n}\n\n.main {\n  width: 100%;\n}\n\n.Signin {\n  margin-bottom: 20px;\n}\n\n.form {\n  width: 100%;\n}\n\n.form-group {\n  margin-bottom: 15px;\n}\n\nlabel {\n  display: block;\n  margin-bottom: 5px;\n}\n\n.input-container {\n  position: relative;\n  display: flex;\n  align-items: center;\n  width: 80%;\n  max-width: 500px;\n}\n\ninput {\n  width: 100%;\n  padding: 10px;\n  padding-left: 40px;\n  font-size: 16px;\n  box-sizing: border-box;\n  border-radius: 4px;\n}\n\n.icon {\n  position: absolute;\n  left: 10px;\n  top: 50%;\n  transform: translateY(-50%);\n  color: #aaa;\n}\n\nbutton {\n  padding: 10px 15px;\n  border: none;\n  background-color: #007BFF;\n  color: white;\n  cursor: pointer;\n  border-radius: 4px;\n  display: flex;\n  align-items: center;\n}\n\nbutton i {\n  margin-left: 10px;\n}\n\nbutton:hover {\n  background-color: #0056b3;\n}\n\n.dotted-line {\n  width: 80%;\n  max-width: 500px;\n  border: none;\n  border-top: 1px dotted #ccc;\n  margin: 20px 0;\n}\n\n.login-link {\n  color: #007BFF;\n  text-decoration: none;\n}\n\n.login-link:hover {\n  text-decoration: underline;\n}\n</style>\n"],"mappings":"AAoDA,OAAOA,KAAI,MAAO,OAAO,EAAE;;AAE3B,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJF,IAAI,EAAE,EAAE;QACRG,MAAM,EAAE,EAAE;QACVC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ,CAAC;MACDC,QAAQ,EAAE,EAAE;MAAE;MACdC,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB;MACA,IAAI,CAACH,cAAa,GAAI,EAAE;MACxB,IAAI,CAACC,YAAW,GAAI,EAAE;;MAEtB;MACA,IAAI,CAAC,IAAI,CAACN,IAAI,CAACF,IAAG,IAAK,CAAC,IAAI,CAACE,IAAI,CAACC,MAAK,IAAK,CAAC,IAAI,CAACD,IAAI,CAACE,KAAI,IAAK,CAAC,IAAI,CAACF,IAAI,CAACG,QAAO,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;QACrG,IAAI,CAACE,YAAW,GAAI,0DAA0D;QAC9E;MACF;MAEA,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMZ,KAAK,CAACa,IAAI,CAAC,wCAAwC,EAAE;UAC1EC,SAAS,EAAE,IAAI,CAACX,IAAI,CAACF,IAAI;UACzBc,UAAU,EAAE,IAAI,CAACZ,IAAI,CAACE,KAAK;UAC3BW,aAAa,EAAE,IAAI,CAACb,IAAI,CAACG,QAAQ;UACjCW,SAAS,EAAE,IAAI,CAACV;QAClB,CAAC,CAAC;QACF,IAAI,CAACC,cAAa,GAAI,kCAAkC;QACxDU,OAAO,CAACC,GAAG,CAAC,WAAW,EAAEP,QAAQ,CAACV,IAAI,CAAC;QACvC;QACA;MACF,EAAE,OAAOkB,KAAK,EAAE;QACd,IAAIA,KAAK,CAACR,QAAQ,EAAE;UAClBM,OAAO,CAACE,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAACR,QAAQ,CAACV,IAAI,CAAC;UAC1DgB,OAAO,CAACE,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAACR,QAAQ,CAACS,MAAM,CAAC;UAC9DH,OAAO,CAACE,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAACR,QAAQ,CAACU,OAAO,CAAC;UAChE,IAAI,CAACb,YAAW,GAAI,SAAQ,IAAKW,KAAK,CAACR,QAAQ,CAACV,IAAI,CAACa,UAAS,GAAIK,KAAK,CAACR,QAAQ,CAACV,IAAI,CAACa,UAAU,CAAC,CAAC,IAAI,mBAAmB,CAAC;QAC5H,OAAO,IAAIK,KAAK,CAACG,OAAO,EAAE;UACxBL,OAAO,CAACE,KAAK,CAAC,qBAAqB,EAAEA,KAAK,CAACG,OAAO,CAAC;UACnD,IAAI,CAACd,YAAW,GAAI,yCAAyC;QAC/D,OAAO;UACLS,OAAO,CAACE,KAAK,CAAC,gBAAgB,EAAEA,KAAK,CAACI,OAAO,CAAC;UAC9C,IAAI,CAACf,YAAW,GAAI,SAAQ,GAAIW,K