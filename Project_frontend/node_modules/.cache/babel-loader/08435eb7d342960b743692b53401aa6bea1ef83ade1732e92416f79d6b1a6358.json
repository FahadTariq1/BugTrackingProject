{"ast":null,"code":"import SingleProject from './SingleProject.vue';\nexport default {\n  name: 'MultipleProjects',\n  components: {\n    SingleProject\n  },\n  data() {\n    return {\n      projects: [] // To store the fetched projects\n    };\n  },\n  computed: {\n    firstThreeProjects() {\n      return this.projects.slice(0, 3);\n    },\n    remainingProjects() {\n      return this.projects.slice(3);\n    }\n  },\n  mounted() {\n    // Make an API call to fetch the data\n    fetch('http://localhost:8000/Projects/allprojects/').then(response => response.json()).then(data => {\n      console.log('Projects data:', data);\n      this.projects = data; // Assign the fetched data to projects\n    }).catch(error => {\n      console.error('Error fetching projects data:', error);\n    });\n  }\n};","map":{"version":3,"names":["SingleProject","name","components","data","projects","computed","firstThreeProjects","slice","remainingProjects","mounted","fetch","then","response","json","console","log","catch","error"],"sources":["/home/ali/BugTrackerProject/Frontend/bugtrackingfrontend/src/components/Multipleprojects.vue"],"sourcesContent":["<template>\n  <div class=\"multiple-projects-container\">\n    <div v-if=\"projects.length > 0\">\n      <div class=\"grid-column\">\n        <SingleProject\n          v-for=\"(project, index) in firstThreeProjects\"\n          :key=\"index\"\n          :project=\"project\"\n        />\n      </div>\n      <div v-if=\"projects.length > 3\" class=\"grid-row\">\n        <SingleProject\n          v-for=\"(project, index) in remainingProjects\"\n          :key=\"index + 3\"\n          :project=\"project\"\n        />\n      </div>\n    </div>\n  </div>\n</template>\n  \n<script>\nimport SingleProject from './SingleProject.vue';\n\nexport default {\n  name: 'MultipleProjects',\n  components: {\n    SingleProject,\n  },\n  data() {\n    return {\n      projects: [], // To store the fetched projects\n    };\n  },\n  computed: {\n    firstThreeProjects() {\n      return this.projects.slice(0, 3);\n    },\n    remainingProjects() {\n      return this.projects.slice(3);\n    },\n  },\n  mounted() {\n    // Make an API call to fetch the data\n    fetch('http://localhost:8000/Projects/allprojects/')\n      .then(response => response.json())\n      .then(data => {\n        console.log('Projects data:', data);\n        this.projects = data; // Assign the fetched data to projects\n      })\n      .catch(error => {\n        console.error('Error fetching projects data:', error);\n      });\n  },\n};\n</script>\n  \n<style scoped>\n.multiple-projects-container {\n  margin-left: 20px;\n  margin-top: 50px;\n}\n\n.grid-column {\n  display: flex;\n  flex-direction: column;\n  gap: 20px; /* Adjust spacing between components */\n}\n\n.grid-row {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px; /* Adjust spacing between components */\n  margin-top: 20px; /* Add spacing between the column and row */\n}\n</style>\n"],"mappings":"AAsBA,OAAOA,aAAY,MAAO,qBAAqB;AAE/C,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE,CAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,kBAAkBA,CAAA,EAAG;MACnB,OAAO,IAAI,CAACF,QAAQ,CAACG,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC;IAClC,CAAC;IACDC,iBAAiBA,CAAA,EAAG;MAClB,OAAO,IAAI,CAACJ,QAAQ,CAACG,KAAK,CAAC,CAAC,CAAC;IAC/B;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR;IACAC,KAAK,CAAC,6CAA6C,EAChDC,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACR,IAAG,IAAK;MACZW,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEZ,IAAI,CAAC;MACnC,IAAI,CAACC,QAAO,GAAID,IAAI,EAAE;IACxB,CAAC,EACAa,KAAK,CAACC,KAAI,IAAK;MACdH,OAAO,CAACG,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;IACvD,CAAC,CAAC;EACN;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}