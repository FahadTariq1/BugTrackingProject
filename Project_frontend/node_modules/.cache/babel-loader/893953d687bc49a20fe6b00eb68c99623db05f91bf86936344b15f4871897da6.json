{"ast":null,"code":"import axios from 'axios';\nimport FileUpload from 'primevue/fileupload';\nimport Button from 'primevue/button';\nexport default {\n  name: 'PopUpNewProject',\n  components: {\n    FileUpload,\n    Button\n  },\n  props: {\n    isVisible: {\n      type: Boolean,\n      required: true\n    }\n  },\n  data() {\n    return {\n      projectName: '',\n      projectDescription: ''\n    };\n  },\n  methods: {\n    closeModal() {\n      this.$emit('update:isVisible', false);\n    },\n    async submitForm() {\n      try {\n        const response = await axios.post('http://localhost:8000/Projects/create/', {\n          project_name: this.projectName,\n          project_description: this.projectDescription\n        });\n        console.log('Project added successfully:', response.data);\n        this.closeModal();\n        this.$emit('project-added');\n      } catch (error) {\n        console.error('Error adding project:', error);\n      }\n    },\n    remove() {\n      this.file = null; // Reset the file property\n      this.$refs.fileupload.clear(); // Clear the FileUpload input\n      console.log(\"File removed\");\n    }\n  }\n};","map":{"version":3,"names":["axios","FileUpload","Button","name","components","props","isVisible","type","Boolean","required","data","projectName","projectDescription","methods","closeModal","$emit","submitForm","response","post","project_name","project_description","console","log","error","remove","file","$refs","fileupload","clear"],"sources":["/home/ali/BugTrackerProject/Frontend/bugtrackingfrontend/src/components/PopUpNewProject.vue"],"sourcesContent":["<template>\n    <div class=\"modal\" v-if=\"isVisible\" @click.self=\"closeModal\">\n      <div class=\"modal-content\">\n        <span class=\"close\" @click=\"closeModal\">&times;</span>\n        <h2>Add New Project</h2>\n        <form @submit.prevent=\"submitForm\">\n          <div class=\"form-group\">\n            <label for=\"project-name\">Project Name</label>\n            <input \n              type=\"text\" \n              id=\"project-name\" \n              v-model=\"projectName\" \n              placeholder=\"Enter project name\" \n              required\n            />\n          </div>\n          <div class=\"form-group\">\n            <label for=\"project-description\">Description</label>\n            <textarea \n              id=\"project-description\" \n              v-model=\"projectDescription\" \n              placeholder=\"Enter project description\" \n              required\n            ></textarea>\n          </div>\n          <div class=\"file-upload-container\">\n            <FileUpload ref=\"fileupload\" mode=\"basic\" name=\"file\" url=\"/api/upload\" accept=\"image/png, image/gif\" :maxFileSize=\"1000000\" @select=\"onSelect\" />\n            <div class=\"file-upload-container2\">\n              <Button label=\"Remove\" @click=\"remove\" severity=\"secondary\" />\n            </div>\n          </div>\n          <button type=\"submit\">Submit</button>\n        </form>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import axios from 'axios';\n  import FileUpload from 'primevue/fileupload';\nimport Button from 'primevue/button';\n  export default {\n    name: 'PopUpNewProject',\n    components:{\n      FileUpload,\n      Button\n    },\n    props: {\n      isVisible: {\n        type: Boolean,\n        required: true\n      }\n    },\n    data() {\n      return {\n        projectName: '',\n        projectDescription: ''\n      };\n    },\n    methods: {\n      closeModal() {\n        this.$emit('update:isVisible', false);\n      },\n      async submitForm() {\n        try {\n          const response = await axios.post('http://localhost:8000/Projects/create/', {\n            project_name: this.projectName,\n            project_description: this.projectDescription\n          });\n          console.log('Project added successfully:', response.data);\n          this.closeModal();\n          this.$emit('project-added');\n        } catch (error) {\n          console.error('Error adding project:', error);\n        }\n      },\n      remove() {\n      this.file = null; // Reset the file property\n      this.$refs.fileupload.clear(); // Clear the FileUpload input\n      console.log(\"File removed\");\n    },\n    }\n  };\n  </script>\n  \n  <style scoped>\n  .modal {\n    position: fixed;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    background-color: rgba(0, 0, 0, 0.5);\n    display: flex;\n    align-items: center;\n    justify-content: center;\n  }\n  \n  .modal-content {\n    background-color: white;\n    padding: 20px;\n    border-radius: 5px;\n    width: 400px;\n    max-width: 90%;\n    position: relative;\n  }\n  \n  .close {\n    position: absolute;\n    top: 10px;\n    right: 10px;\n    font-size: 20px;\n    cursor: pointer;\n  }\n  \n  h2 {\n    margin: 0;\n    font-size: 18px;\n  }\n  \n  .form-group {\n    margin-bottom: 15px;\n  }\n  \n  label {\n    display: block;\n    margin-bottom: 5px;\n  }\n  \n  input, textarea {\n    width: 100%;\n    padding: 10px;\n    border: 1px solid #ccc;\n    border-radius: 5px;\n  }\n  \n  button {\n    padding: 10px 20px;\n    font-size: 16px;\n    color: #fff;\n    background-color: #007bff;\n    border: none;\n    border-radius: 5px;\n    cursor: pointer;\n  }\n  \n  button:hover {\n    background-color: #0056b3;\n  }\n  </style>\n  "],"mappings":"AAsCE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,UAAS,MAAO,qBAAqB;AAC9C,OAAOC,MAAK,MAAO,iBAAiB;AAClC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAC;IACTH,UAAU;IACVC;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACC,KAAK,CAAC,kBAAkB,EAAE,KAAK,CAAC;IACvC,CAAC;IACD,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMjB,KAAK,CAACkB,IAAI,CAAC,wCAAwC,EAAE;UAC1EC,YAAY,EAAE,IAAI,CAACR